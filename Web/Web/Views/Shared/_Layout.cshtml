<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Resilient Coating Solutions</title>
    <meta name="keywords" content="Resin8 Epoxy Resin Commercial Coating" />
    <meta name="description" content="Resin8. Highly Resilient Epoxy Coating. Low VOC. ASTM testing. 1-hour fast cure. Water cleanable. 25 year performance legacy for Commercial CIPP Resin8 Commercial products. " />
    <meta name="author" content="tosca ragnini" />

    <link rel="shortcut icon" href="../img/resin8/resin8-favicon.png" />
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,300,600,400italic' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Roboto:400,700,500|Roboto+Condensed:400,700' rel='stylesheet' type='text/css'>
    <link href="~/fonts/font-awesome.min.css" rel="stylesheet" />
    <link href="~/css/bootstrap.min.css" rel="stylesheet" type='text/css' />
    <link href="~/css/hover-dropdown-menu.css" rel="stylesheet" type='text/css' />
    <link href="~/css/revolution-slider.css" rel="stylesheet" type='text/css' />
    <link href="~/rs-plugin/css/settings.css" rel="stylesheet" type='text/css' />
    <link href="~/css/animate.min.css" rel="stylesheet" type='text/css' />
    <link href="~/css/owl/owl.carousel.css" rel="stylesheet" type='text/css' />
    <link href="~/css/owl/owl.theme.css" rel="stylesheet" type='text/css' />
    <link href="~/css/owl/owl.transitions.css" rel="stylesheet" type='text/css' />
    <link href="~/css/prettyPhoto.css" rel="stylesheet" type='text/css' />
    <link href="~/css/style.css" rel="stylesheet" type='text/css' />
    <link href="~/css/responsive.css" rel="stylesheet" type='text/css' />
    <link href="~/css/color.css" rel="stylesheet" type='text/css' />
   <!-Styles.Render("~/Content/css")
    Scripts.Render("~/bundles/modernizr")-->
</head>
<body>
    <div id="page">
        <div id="pageloader">
            <div class="loader-item fa fa-spin text-color"></div>
        </div>
        <div id="top-bar" class="top-bar-section top-bar-bg-color">
    <div class="container">
        <div class="row">
            <div class="col-sm-12">
                <div class="top-contact link-hover-black right">
                    <a href="tel:773-551-3633">
                        <i class="fa fa-phone"></i> 773.551.3633
                    </a>
                    <a href="mailto:roger@resin-8.com">
                        <i class="fa fa-envelope"></i>info@resin-8.com
                    </a>
                </div>
            </div>
        </div>
    </div>
</div>
        <header id="sticker" class="sticky-navigation">
            <!-- Sticky Menu -->
            <div class="sticky-menu relative">
                <!-- navbar -->
                <div id="navigation" class="navbar navbar-default navbar-bg-light" role="navigation">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="navbar-header">
                                    <!-- Button For Responsive toggle -->
                                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                                        <span class="sr-only">Toggle navigation</span>
                                        <span class="icon-bar"></span>
                                        <span class="icon-bar"></span>
                                        <span class="icon-bar"></span>
                                    </button>
                                    <a class="navbar-brand" href="#home"><img class="site_logo" alt="Site Logo" src="../img/resin8/resin8-logo.png"></a>
                                </div>
                                <div id="topnav" class="navbar-collapse collapse">
                                    <ul class="nav navbar-nav">
                                        <li> <a href="#home" class="scroll">Home</a></li>
                                        <li><a href="#about-us" class="scroll">About Us</a> <li>
                                     <!--  <li><a href="#strengths" class="scroll">Strengths</a></li>--> 
                                        <li><a href="#markets" class="scroll">Markets</a></li>
                                        <li><a href="#products" class="scroll">Products</a></li>                                         
                                   <li> <a href="#contact" class="scroll">Contact us</a></li>
                                     </ul>                                                             
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <section class="slider" id="home">
            <div class="tp-banner">
                <ul>
                    <li data-delay="7000" data-transition="fade" data-slotamount="7" data-masterspeed="2000">
                        <div class="elements">
                            <div class="tp-caption tp-resizeme lft" data-x="0" data-y="0" data-speed="1000" data-start="2040" data-kenburns="on" data-bgfit="80" data-bgfitend="100" data-bgpositionend="right bottom" data-easing="Power4.easeOut" data-endspeed="500" data-endeasing="Power1.easeIn" style="z-index: 4; top: 0px">
                                <img src="../img/resin8/resin8slide-01.png" alt="resin8 coating solutions" width="661" height="715" />
                            </div>
                            <div class="tp-caption tp-resizeme lfr" data-x="502" data-y="110" data-speed="1000" data-start="2540" data-kenburns="on" data-easing="Power4.easeOut" data-endspeed="500" data-endeasing="Power1.easeIn" style="z-index: 4">
                                <img src="../img/resin8/hex6.png" alt="resin8 coating systems" />
                            </div>
                            <h4 class="tp-caption tp-resizeme lft skewtotop large_text title" data-x="402" data-y="40" data-speed="1000" data-start="1700" data-easing="Power4.easeOut" data-endspeed="500" data-endeasing="Power1.easeIn" style="z-index: 5">
                                <strong>
                                    Industrial Epoxy Coating
                                </strong>
                            </h4>
                            <h4 class="tp-caption medium_text tp-resizeme lft skewtotop" data-x="462" data-y="130" data-speed="1000" data-start="2040" data-easing="Power4.easeOut" data-endspeed="500" data-endeasing="Power1.easeIn" style="z-index: 5">
                                <strong>CIPP Pipeliner</strong>
                            </h4>
                            <h4 class="tp-caption medium_text tp-resizeme lfr skewtoright" data-x="482" data-y="180" data-speed="1000" data-start="2600" data-easing="Power4.easeOut" data-endspeed="500" data-endeasing="Power1.easeIn" style="max-width: 600px; z-index: 5">
                                <strong>Floors and walls</strong>
                            </h4>
                            <h4 class="tp-caption medium_text tp-resizeme lfr skewtotop" data-x="502" data-y="230" data-speed="1000" data-start="3000" data-easing="Power4.easeOut" data-endspeed="500" data-endeasing="Power1.easeIn" style="max-width: 600px; z-index: 5">
                                <strong>Primers</strong>
                            </h4>
                            <h4 class="tp-caption medium_text tp-resizeme lfr skewtoright" data-x="522" data-y="280" data-speed="1000" data-start="3500" data-easing="Power4.easeOut" data-endspeed="500" data-endeasing="Power1.easeIn" style="max-width: 600px; z-index: 5">
                                <strong>Crack Filler</strong>
                            </h4>
                            <h4 class="tp-caption medium_text tp-resizeme lfr skewtoright" data-x="542" data-y="330" data-speed="1000" data-start="4000" data-easing="Power4.easeOut" data-endspeed="500" data-endeasing="Power1.easeIn" style="max-width: 600px; z-index: 5">
                                <strong>Manholes</strong>
                            </h4>
                            <h4 class="tp-caption medium_text tp-resizeme lfr skewtoright" data-x="562" data-y="380" data-speed="1000" data-start="4500" data-easing="Power4.easeOut" data-endspeed="500" data-endeasing="Power1.easeIn" style="max-width: 600px; z-index: 5">
                                <strong>Tile Grout</strong>
                            </h4>
                        </div>
                        <img src="../img/resin8/grey_floor.jpg" alt="grey floor background" data-bgfit="cover" data-bgpositionend="center top" data-bgrepeat="no-repeat" />
                    </li>
                </ul>
                <div class="tp-bannertimer"></div>
            </div>
        </section>
 
                 @RenderBody()
                
         <hr />       
        <footer id="contact" class="footer-section footer-bg-color">
            <div class="footer-widget">
                <div class="container">
                    <div class="row">
                        <div class="col-xs-12 col-sm-6  widget bottom-xs-pad-20">
                            <div class="widget-title">
                                <h4 class="white">RESIN8, Incorporated</h4>
                            </div>
                            <h5 class="white">Chicago, Illinois, USA</h5>
                            <h5 class="white">Phone- 773.551.3633</h5>
                        </div>
                        <div class="col-xs-12 col-sm-6 widget">
                            <div class="widget-title">
                                <h4 class="white">Email</h4>
                            </div>
                            <h5 class="white">roger@resin-8.com</h5>
                            <h5 class="white">chuck@resin-8.com</h5>

                            <!--contact form-->

                        </div>
                    </div>
                </div>
            </div>
            <div class="copyright">
                <div class="container">
                    <div class="row">
                        <div class="col-xs-10 col-sm-6 col-md-6">
                            &copy; @DateTime.Now.Year - Resin8 - Resilient Coatings Solutions.
                            <br />
                        </div>
                        <div class="col-xs-2  col-sm-6 col-md-6 text-right page-scroll gray-bg icons-circle i-3x">
                            <a href="#home">
                                <i class="glyphicon glyphicon-arrow-up"></i>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </footer>

                       
        <script type="text/javascript" src="~/js/jquery.min.js"></script>
        <script type="text/javascript" src="~/js/bootstrap.min.js"></script>
        <script type="text/javascript" src="~/js/hover-dropdown-menu.js"></script>
        <script type="text/javascript" src="~/js/jquery.hover-dropdown-menu-addon.js"></script>
        <script type="text/javascript" src="~/js/jquery.easing.1.3.js"></script>
        <script type="text/javascript" src="~/js/jquery.sticky.js"></script>
        <script type="text/javascript" src="~/js/bootstrapValidator.min.js"></script>
        <script type="text/javascript" src="~/rs-plugin/js/jquery.themepunch.tools.min.js"></script>
        <script type="text/javascript" src="~/rs-plugin/js/jquery.themepunch.revolution.min.js"></script>
        <script type="text/javascript" src="~/js/revolution-custom.js"></script>
        <script type="text/javascript" src="~/js/jquery.mixitup.min.js"></script>
        <script type="text/javascript" src="~/js/jquery.appear.js"></script>
        <script type="text/javascript" src="~/js/effect.js"></script>
        <script type="text/javascript" src="~/js/owl.carousel.min.js"></script>
        <script type="text/javascript" src="~/js/jquery.prettyPhoto.js"></script>
        <script type="text/javascript" src="~/js/jquery.parallax-1.1.3.js"></script>
        <script type="text/javascript" src="~/js/custom.js"></script>
                    <!--   Scripts.Render("~/bundles/jquery") Scripts.Render("~/bundles/bootstrap")--> 
             @RenderSection("scripts", required: false)
        </div>
</body>
</html>
